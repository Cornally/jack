{
  "name": "jack-strap",
  "version": "0.0.20",
  "description": "Lightweight/portable styling engine",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "node copy-sass.js",
    "build": "sass --update src:dist --style expanded --sourcemap=none && sass --update src:dist/min --style compressed --sourcemap=none"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cornally/jack.git"
  },
  "keywords": [
    "styling",
    "framework",
    "jack",
    "sass",
    "scss",
    "flexbox",
    "flex"
  ],
  "author": "@Cornally",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cornally/jack/issues"
  },
  "homepage": "https://github.com/Cornally/jack#readme",
  "dependencies": {
    "express": "^4.15.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "cpx": "^1.5.0",
    "node-sass": "^4.5.2",
    "postcss-cli": "^3.1.1"
  }
}
